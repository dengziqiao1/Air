            <enum name="center_top" value="3"/>
            <enum name="center" value="4"/>
            <enum name="center_bottom" value="5"/>
            <enum name="right_top" value="6"/>
            <enum name="right_center" value="7"/>
            <enum name="right_bottom" value="8"/>
