            <enum name="left_top" value="0"/>
            <enum name="left_center" value="1"/>
            <enum name="left_bottom" value="2"/>
            <enum name="center_top" value="3"/>
            <enum name="center" value="4"/>
            <enum name="center_bottom" value="5"/>
